    "pinia": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vue": "^3.3.4",
    "vue-tsc": "^1.6.5",
    "pinia": "2.0.33"
