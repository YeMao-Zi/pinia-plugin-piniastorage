{
  "name": "pinia-plugin-piniastorage",
  "type": "module",
  "version": "1.1.0",
  "description": "pinia 的持久化插件,支持自定义存储方式",
  "author": {
    "name": "YeMao-Zi"
  },
  "license": "MIT",
  "homepage": "https://github.com/YeMao-Zi/pinia-plugin-piniastorage#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/YeMao-Zi/pinia-plugin-piniastorage"
  },
  "keywords": [
    "pinia",
    "pinia-plugin",
    "storage"
  ],
  "exports": {
